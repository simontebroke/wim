{
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "rootDir": "src",
    "collectCoverageFrom": [
        "**/*.{js,jsx,ts,tsx}",
        "!**/node_modules/**",
        "!**/__tests__/**"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "testMatch": ["**/__tests__/**/*.test.(js|ts)?(x)"]
}
